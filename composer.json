{
  "name": "dwo/process-processor",
  "description": "a processor to process concurrent symfony processes",
  "keywords": ["process", "processor", "queue", "concurrent"],
  "homepage": "https://github.com/davewwww",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dave Www",
      "email": "davewwwo@gmail.com",
      "homepage": "https://github.com/davewwww"
    }
  ],
  "require": {
    "php": ">=7.3",
    "symfony/console": "^4.3 || ^5.0 || ^6.0",
    "symfony/process": "^4.3 || ^5.0 || ^6.0",
    "symfony/stopwatch": "^4.3 || ^5.0 || ^6.0"
  },
  "autoload": {
    "psr-4": { "Dwo\\ProcessProcessor\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": {
      "Dwo\\ProcessProcessor\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
